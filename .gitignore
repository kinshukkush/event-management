# Laravel .gitignore

# Dependency directories
/node_modules
/vendor

# Environment files
/.env
/.env.backup
/.env.production

# Build outputs
/public/hot
/public/storage
/public/build
/dist

# Storage files
/storage/*.key
/storage/app/*
!/storage/app/.gitignore
/storage/framework/cache/*
!/storage/framework/cache/.gitignore
/storage/framework/sessions/*
!/storage/framework/sessions/.gitignore
/storage/framework/testing/*
!/storage/framework/testing/.gitignore
/storage/framework/views/*
!/storage/framework/views/.gitignore
/storage/logs/*
!/storage/logs/.gitignore

# Laravel specific
Homestead.json
Homestead.yaml
auth.json
npm-debug.log
yarn-error.log
/.fleet
/.idea
/.vscode
.phpunit.result.cache

# OS files
.DS_Store
Thumbs.db
*.swp
*.swo
*~

# IDE files
.idea/
.vscode/
*.sublime-project
*.sublime-workspace

# Cache and logs
*.log
*.cache

# Testing
/.phpunit.cache
.phpunit.result.cache
/coverage
